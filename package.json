{
	"name": "node-fetch",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"convert-vars": "tsx scripts/convert-vars.ts",
		"watch:api": "tsx scripts/watch-api-changes.ts",
		"deploy": "yarn deploy:pages",
		"deploy:pages": "yarn build:frontend && wrangler pages deploy frontend/dist",
		"dev:backend": "wrangler pages dev --port 8787",
		"dev:frontend": "cd frontend && yarn dev",
		"dev": "concurrently \"yarn dev:backend\" \"yarn dev:frontend\"",
		"dev:with-watch": "concurrently \"yarn dev:backend\" \"yarn dev:frontend\" \"yarn watch:api\"",
		"build:frontend": "yarn build:api && cd frontend && yarn build",
		"build:cf-type": "wrangler types",
		"build:api": "tsx scripts/generate-api-client.ts",
		"type-check": "tsc --noEmit && cd frontend && yarn type-check"
	},
	"devDependencies": {
		"@types/jest": "^30.0.0",
		"@types/node": "^24.0.3",
		"concurrently": "^9.2.0",
		"oazapfts": "^6.3.0",
		"tsx": "^4.20.3",
		"typescript": "^5.5.2",
		"wrangler": "^4.16.0"
	},
	"dependencies": {
		"@hono/swagger-ui": "^0.5.2",
		"@hono/zod-openapi": "^0.19.10",
		"@oazapfts/runtime": "^1.0.4",
		"hono": "^4.8.0",
		"yaml": "^2.7.0",
		"zod": "^3.25.67"
	},
	"packageManager": "yarn@4.9.2"
}
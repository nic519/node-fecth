{
	"name": "node-fetch",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"convert-vars": "tsx scripts/convert-vars.ts",
		"deploy": "yarn deploy:pages",
		"deploy:pages": "VITE_API_BASE_URL=https://world.1024.hair yarn build:frontend && wrangler pages deploy public",
		"dev:backend": "wrangler pages dev public --port 8787",
		"dev:frontend": "cd frontend && yarn dev",
		"dev": "concurrently \"yarn dev:backend\" \"yarn dev:frontend\"", 
		"build:frontend": "tsx scripts/build-frontend.ts",
		"build:cf-type": "wrangler types",
		"build:api": "tsx scripts/generate-api-client.ts",
		"type-check": "tsc --noEmit && cd frontend && yarn type-check",
		"clean:build": "tsx scripts/clean-build.ts"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.4",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.0.3",
		"concurrently": "^9.2.0",
		"fs-extra": "^11.3.0",
		"oazapfts": "^6.3.0",
		"tsx": "^4.20.3",
		"typescript": "^5.5.2",
		"wrangler": "^4.16.0"
	},
	"dependencies": {
		"@hono/swagger-ui": "^0.5.2",
		"@hono/zod-openapi": "^0.19.10",
		"@oazapfts/runtime": "^1.0.4",
		"hono": "^4.8.0",
		"yaml": "^2.7.0",
		"zod": "^3.25.67"
	},
	"packageManager": "yarn@4.9.2"
}

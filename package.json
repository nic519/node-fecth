{
	"name": "node-fetch",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"convert-vars": "tsx scripts/convert-vars.ts",
		"generate:api": "tsx scripts/generate-api-client.ts",
		"generate:openapi": "tsx scripts/generate-openapi.ts",
		"watch:api": "tsx scripts/watch-api-changes.ts",
		"deploy": "yarn deploy:pages",
		"deploy:pages": "yarn build:frontend && wrangler pages deploy frontend/dist",
		"dev:backend": "wrangler dev --config wrangler.dev.toml --port 8787",
		"dev:frontend": "cd frontend && yarn dev",
		"dev": "concurrently \"yarn dev:backend\" \"yarn dev:frontend\"",
		"dev:with-watch": "concurrently \"yarn dev:backend\" \"yarn dev:frontend\" \"yarn watch:api\"",
		"build:frontend": "yarn generate:api && cd frontend && yarn build",
		"cf-typegen": "wrangler types",
		"predev": "yarn convert-vars && yarn generate:api && yarn type-check",
		"type-check": "tsc --noEmit"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250214.0",
		"@types/node": "^24.0.3",
		"concurrently": "^9.2.0",
		"ts-node": "^10.9.1",
		"tsx": "^4.20.3",
		"typescript": "^5.5.2",
		"wrangler": "^4.16.0"
	},
	"dependencies": {
		"@hono/swagger-ui": "^0.5.2",
		"@hono/zod-openapi": "^0.19.10",
		"hono": "^4.8.0",
		"yaml": "^2.7.0",
		"zod": "^3.25.67"
	},
	"packageManager": "yarn@4.9.2"
}